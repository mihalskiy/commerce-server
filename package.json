    "autosize": "^4.0.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.15.2",
    "connect-session-sequelize": "^6.0.0",
    "cookie-parser": "^1.4.3",
    "dotenv": "^6.1.0",
    "es6-tween": "^5.3.1",
    "express": "^4.14.0",
    "express-session": "^1.15.6",
    "express-session-sequelize": "^1.1.0",
    "ios-inner-height": "^1.1.0",
    "js-yaml": "^3.11.0",
    "jsonwebtoken": "^8.2.0",
    "lodash": "^4.17.11",
    "morgan": "^1.9.0",
    "nodemailer": "^4.7.0",
    "nodemailer-smtp-transport": "^2.7.4",
    "passport": "^0.4.0",
    "passport-facebook": "^2.1.1",
    "passport-google-oauth": "^1.0.0",
    "pg": "6.1.0",
    "pg-hstore": "^2.3.2",
    "phone": "^2.0.0-rc.1",
    "react": "^16.6.1",
    "react-circle": "^1.1.1",
    "react-dom": "^16.6.1",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.1.1",
    "react-router-dom": "^4.3.1",
