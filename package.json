    "autosize": "^4.0.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "connect-session-sequelize": "^6.0.0",
    "cookie-parser": "^1.4.4",
    "dotenv": "^8.1.0",
    "es6-tween": "^5.5.10",
    "express": "^4.17.1",
    "express-session": "^1.16.2",
    "express-session-sequelize": "^2.1.0",
    "ios-inner-height": "^1.1.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "material-ui": "^0.20.2",
    "material-ui-chip-input": "^1.0.0",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "nodemailer": "^6.3.0",
